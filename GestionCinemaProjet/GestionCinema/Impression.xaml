<Window x:Class="GestionCinema.Impression"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Impression Ticket" Height="150" Width="600"
        Style="{StaticResource Windows}" Initialized="Lancer"
        WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBlock HorizontalAlignment="Center" Text="Ticket en cours d'impression, veuillez patienter ..." Style="{StaticResource StyleText}" Margin="0,15"/>
        <ProgressBar Name="mProgressBar" Height="40" Margin="5"/>
    </StackPanel>
</Window>
